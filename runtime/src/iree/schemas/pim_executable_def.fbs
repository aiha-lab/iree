namespace iree;

// 'PIM Executable'.
file_identifier "PIMX";
file_extension "pimx";

table PIMExecutableDef {
  // A map of entry point ordinals to string names 
  entry_points:[string];

  // PIM code
  code:[uint64];
}
root_type PIMExecutableDef;
